<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Consolas" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="PHP" value="true" />
      </map>
    </option>
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
  </component>
  <component name="LiveEditOptions" chromeAutoDelay="600" chromeUpdateApp="true" chromeUpdateOnChanges="true" nodeAutoDelay="500" nodeUpdateOnChanges="true">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" pattern="\bfixme\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="\b!!!\b.*">
      <option name="FOREGROUND" value="3b6661" />
      <option name="BACKGROUND" value="1d0d17" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="633256" />
      <option name="ERROR_STRIPE_COLOR" value="9b2593" />
    </pattern>
    <filter name="TODO">
      <pattern index="1" />
      <pattern index="2" />
      <pattern index="0" />
    </filter>
  </component>
</application>