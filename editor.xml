<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Consolas" />
    <option name="LINE_SPACING" value="0.9" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="PHP" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="20" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_LEADING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="IS_MOUSE_CLICK_SELECTION_HONORS_CAMEL_WORDS" value="false" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="JSEditorOptions">
    <option name="START_STRING_INTERPOLATION_ON_TYPING" value="true" />
  </component>
  <component name="LiveEditOptions" chromeAutoDelay="600" chromeUpdateApp="true" chromeUpdateOnChanges="true" nodeAutoDelay="500" nodeUpdateOnChanges="true">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" pattern="\bfixme\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="[!]{2,}:.*">
      <option name="FOREGROUND" value="a44949" />
      <option name="BACKGROUND" value="26080c" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="ae318c" />
      <option name="ERROR_STRIPE_COLOR" value="c0227d" />
    </pattern>
    <filter name="TODO">
      <pattern index="1" />
      <pattern index="2" />
      <pattern index="0" />
    </filter>
    <filter name="ALARM">
      <pattern index="2" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="tagTreeHighlightingOpacity" value="15" />
  </component>
</application>