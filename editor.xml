      <option name="FOREGROUND" value="b55275" />
      <option name="BACKGROUND" value="26080c" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="84194f" />
      <option name="ERROR_STRIPE_COLOR" value="c0227d" />
    </pattern>
    <filter name="TODO">
      <pattern index="1" />
      <pattern index="0" />
      <pattern index="2" />
    </filter>
    <filter name="ALARM">
      <pattern index="2" />
    </filter>
