<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_DOC_COMMENTS" value="true" />
  </component>
  <component name="CodeInsightSettings">
    <option name="completionCaseSensitive" value="2" />
    <option name="AUTO_POPUP_PARAMETER_INFO" value="false" />
    <option name="PARAMETER_INFO_DELAY" value="3000" />
    <option name="AUTO_POPUP_COMPLETION_LOOKUP" value="false" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="REFORMAT_ON_PASTE" value="4" />
    <option name="HIGHLIGHT_SCOPE" value="true" />
    <option name="TAB_EXITS_BRACKETS_AND_QUOTES" value="false" />
  </component>
  <component name="DaemonCodeAnalyzerSettings" profile="NB-inspect-light">
    <option name="AUTOREPARSE_DELAY" value="350" />
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="PHP" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.*" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="10" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="false" />
    <option name="SHOW_INSPECTION_WIDGET" value="false" />
    <option name="TOOLTIPS_DELAY_MS" value="400" />
    <option name="CARET_BLINKING_PERIOD" value="400" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_LEADING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="IS_DND_ENABLED" value="false" />
    <option name="BIDI_TEXT_DIRECTION" value="LTR" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledLanguages">
      <set>
        <option value="JSON" />
        <option value="JavaScript" />
        <option value="PHP" />
        <option value="SQL" />
        <option value="TypeScript" />
      </set>
    </option>
    <option name="enabled" value="false" />
    <option name="lastViewedProviderKeyId" value="composer.package.version.hints" />
  </component>
  <component name="PhpCodeFoldingSettings">
    <option name="COLLAPSE_HERE_DOCS" value="true" />
  </component>
  <component name="SqlEditorTabsSettings" SHORTEN_TAB_NAMES="false">
    <option name="shortenTabNames" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="question" pattern="\btodo\b.*" />
    <pattern case-sensitive="true" pattern="\bfixme\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="[!]{2,}(.*?)[!]{2,}">
      <option name="FOREGROUND" value="58c5d7" />
      <option name="BACKGROUND" value="18373f" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="46272" />
      <option name="ERROR_STRIPE_COLOR" value="8b8d7" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="tagTreeHighlightingLevelCount" value="5" />
  </component>
</application>